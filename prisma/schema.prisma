generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Wallet {
  id Int @id @default(autoincrement())

  address    String   @unique
  signers    String[] @default([])
  isDeployed Boolean  @default(false)

  salt         String
  transactions Transaction[]
}